<h1>Login</h1>

<fieldset>
<legend>Enter Your Credentials</legend>

<%= form_for User.new do |f| %> 
    <p>
    <%= f.label :username %> 
    <%= f.text_field :username %>
    </p>

    <p>
    <%= f.label :password %> 
    <%= f.password_field :password %>
    </p>

    
    <%= f.submit "Login" %> 
<% end %> 

</fieldset>
