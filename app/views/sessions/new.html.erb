<h1>Create a Profile</h1>

<fieldset>
<legend>Basic User Information</legend>

<%= form_for User.new do |f| %> 
    <p>
    <%= f.label :email %> 
    <%= f.text_field :email %>
    </p>

    <p>
    <%= f.label :password %> 
    <%= f.password_field :password %>
    </p>

    


    <%= f.submit %> 
<% end %> 

</fieldset>
